{
  "name": "victormukha/zen-sdk",
  "description": "PHP SDK for Zen Payments API",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0",
    "vlucas/phpdotenv": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "ZenSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZenSdk\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
